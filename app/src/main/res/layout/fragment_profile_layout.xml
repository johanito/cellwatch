<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.example.niden.cellwatchsharing.activities.TechnicianActivity">


    <include layout="@layout/element_profile_cover"

        android:layout_height="wrap_content"
        android:layout_width="match_parent" />

    <Button
        android:id="@+id/btn_num_task"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/style_button_sign_in_round"
        android:text="Task"
        android:textColor="#ffffff" />

    <include
        layout="@layout/element_profile_info"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="25dp" />

</LinearLayout>
